  "license": "MIT",
  "homepage": "https://github.com/esatterwhite/skyring/tree/master/packages/tcp-transport#readme",
  "engines": {
    "node": ">= 8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/esatterwhite/skyring.git",
    "directory": "packages/tcp-transport"
  },
  "bugs": {
    "email": "esatterwhite@wi.rr.com",
    "url": "https://github.com/esatterwhite/skyring/issues"
  },
  "dependencies": {
